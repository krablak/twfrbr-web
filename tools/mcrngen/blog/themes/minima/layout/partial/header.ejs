<div class="row">
  <div class="twelve columns">
    <img class="logo" src="<%= config.root %>/images/logov5-web.svg" />
    <h1>
      <%= theme.title %>
    </h1>
  </div>

  <div class="twelve columns">
    <div class="row">
      <div class="nine columns left">
        <a href="<%= config.root %>">Home</a>
        <% for (name in theme.menu) { %>
          <% if (theme.menu[name]) { %>
            <a href="<%- url_for(theme.menu[name]) %>" class="ml">
              <%- name %>
            </a>
            <% } %>
              <% } %>
      </div>
    </div>
    <hr style="margin-bottom: 2.6rem">
  </div>
</div>